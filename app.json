{
  "expo": {
    "name": "Batista Livre Acesso",
    "slug": "IBLA",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.batistalivreacesso.portallivreacesso"
    },
    "android": {   
      "package": "br.com.batistalivreacesso.portal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
